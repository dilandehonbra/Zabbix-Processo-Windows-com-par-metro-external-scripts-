IPHOST=$1
PORTNUM=$2
PARAM=$3


GETPID="$(zabbix_get -s ${IPHOST} -p ${PORTNUM}  -k wmi.getall["root\cimv2,select * from Win32_Process where CommandLine like '${PARAM}' "] | jq | awk '/"ProcessId":/ {print $2}' | sed 's/,//g')" 


zabbix_get -s ${IPHOST} -p ${PORTNUM}  -k wmi.getall["root\cimv2,SELECT * FROM Win32_PerfFormattedData_PerfProc_Process where IDProcess='${GETPID}'"]  | jq | awk '/"PercentProcessorTime":/ {print $2}' | sed 's/[[:punct:]]//g'
